<?xml version="1.0" encoding="utf-8"?>
<manifest
    package="com.cafelinear.knightsoflambdacalculus"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".HelloWorldActivity"
            android:label="@string/title_activity_hello_world"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.cafelinear.knightsoflambdacalculus.MainActivity"/>
        </activity>
        <activity
            android:name=".HelloWorld2Activity"
            android:label="@string/title_activity_hello_world2"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.cafelinear.knightsoflambdacalculus.MainActivity"/>
        </activity>
        <activity
            android:name=".OperatorsActivity"
            android:label="@string/title_activity_operators"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.cafelinear.knightsoflambdacalculus.MainActivity"/>
        </activity>
        <activity
            android:name=".TextWatcherActivity"
            android:label="@string/title_activity_git_hub"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.cafelinear.knightsoflambdacalculus.MainActivity"/>
        </activity>
        <activity
            android:name=".GitHubActivity"
            android:label="@string/title_activity_git_hub2"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.cafelinear.knightsoflambdacalculus.MainActivity"/>
        </activity>
    </application>

</manifest>
